program makecode
  use fint
  real(kind=8) :: agr,ft,res
  integer :: l1,l2,l3,m1,m2,m3,n1,n2,n3,n4
  call cint(1,2,0,1,2,3,4,-1,0,-3,agr)
  write(*,*) agr
  call cint(1,2,0,1,2,3,4,-1,1,-3,agr)
  write(*,*) agr

  select case(n3)
  case(0)
     select case(n4)
     case(0)
        res = 0d0
     case(1)
        if((m1+m2+m3 .eq. -1) .or. (m1+m2+m3 .eq. -1)) then
           ft = pc.pi
           call cint(l1,l2,l3,m1,m2,m3,n1,n2,n3,n4,agr)
           res = ft*agr
        end if
     case(2)
        if((m1+m2+m3 .eq. -2) .or. (m1+m2+m3 .eq. -2)) then
           ft = pc.pi*0.5d0
           call cint(l1,l2,l3,m1,m2,m3,n1,n2,n3,n4,agr)
           res = ft*agr
        end if
        if((m1+m2+m3 .eq. 0)) then
           ft = pc.pi
           call cint(l1,l2,l3,m1,m2,m3,n1,n2,n3,n4,agr)
           res = ft*agr
        end if
     end select
  case(1)
     res = 0d0
  case(2)
     select case(n4)
     case(0)
        if((m1+m2+m3 .eq. -2) .or. (m1+m2+m3 .eq. -2)) then
           ft = pc.pi*0.5d0
           call cint(l1,l2,l3,m1,m2,m3,n1,n2,n3,n4,agr)
           res = ft*agr
        end if
        if((m1+m2+m3 .eq. 0)) then
           ft = pc.pi
           call cint(l1,l2,l3,m1,m2,m3,n1,n2,n3,n4,agr)
           res = ft*agr
        end if
     case(1)
        if((m1+m2+m3 .eq. -3) .or. (m1+m2+m3 .eq. -3)) then
           ft = -pc.pi*0.25d0
           call cint(l1,l2,l3,m1,m2,m3,n1,n2,n3,n4,agr)
           res = ft*agr
        end if
        if((m1+m2+m3 .eq. -1) .or. (m1+m2+m3 .eq. -1)) then
           ft = pc.pi*0.25d0
           call cint(l1,l2,l3,m1,m2,m3,n1,n2,n3,n4,agr)
           res = ft*agr
        end if
     case(2)
          if((m1+m2+m3 .eq. -4) .or. (m1+m2+m3 .eq. -4)) then
           ft = -pc.pi*0.125d0
           call cint(l1,l2,l3,m1,m2,m3,n1,n2,n3,n4,agr)
           res = ft*agr
        end if
        if((m1+m2+m3 .eq. 0)) then
           ft = pc.pi*0.125d0
           call cint(l1,l2,l3,m1,m2,m3,n1,n2,n3,n4,agr)
           res = ft*agr
        end if
     end select
  end select
end program makecode
